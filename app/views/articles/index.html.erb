<h1>Listing of all the Articles</h1>
<p>
<%= link_to "Create New Article", new_article_path %>
</p>

<table>
  <tr>
    <th>Title</th>
    <th>Description</th>
  </tr>
  
    <% @articles.each do |anything| %>
  <tr>
    <td><%= anything.title %></td>
    <td><%= anything.description %></td>
    <td><%= link_to "Edit" , edit_article_path(anything) %></td>
    <td><%= link_to "Show" , article_path(anything) %></td>
  </tr>
  <% end %>

</table>